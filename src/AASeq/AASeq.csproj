<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net5.0</TargetFramework>
    <AssemblyName>AASeq</AssemblyName>
    <RootNamespace>AASeq.Console</RootNamespace>
  </PropertyGroup>

  <PropertyGroup>
    <LangVersion>latest</LangVersion>
    <Nullable>enable</Nullable>
    <NoWarn>CA1303</NoWarn>
    <WarningsAsErrors>CS8625</WarningsAsErrors>
  </PropertyGroup>

  <PropertyGroup>
    <Product>AASeq</Product>
    <Description>Protocol simulator.</Description>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\AASeq.Core\AASeq.Core.csproj" />
    <ProjectReference Include="..\AASeq.Engine\AASeq.Engine.csproj" />
    <ProjectReference Include="..\AASeq.Plugin\AASeq.Plugin.csproj" />
  </ItemGroup>

</Project>
