<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <OutputType>Library</OutputType>
        <TargetFramework>net5.0</TargetFramework>
        <AssemblyName>Tipfeler.Plugin.Ping</AssemblyName>
        <RootNamespace>Tipfeler.Plugin.Ping</RootNamespace>
    </PropertyGroup>

    <PropertyGroup>
        <LangVersion>latest</LangVersion>
        <Nullable>disable</Nullable>
        <NoWarn>CA1303</NoWarn>
        <WarningsAsErrors>CS8625</WarningsAsErrors>
    </PropertyGroup>

    <PropertyGroup>
        <Product>Tipfeler</Product>
        <Description>Ping protocol.</Description>
    </PropertyGroup>

    <ItemGroup>
        <ProjectReference Include="..\Tipfeler.Core\Tipfeler.Core.csproj" />
        <ProjectReference Include="..\Tipfeler.Plugin\Tipfeler.Plugin.csproj" />
    </ItemGroup>

    <ItemGroup>
        <CodeAnalysisDictionary Include="..\CodeAnalysisDictionary.xml">
            <Link>Properties\CodeAnalysisDictionary.xml</Link>
        </CodeAnalysisDictionary>
    </ItemGroup>

</Project>
