#!/usr/bin/env aaseq

@Me {
    Timeout "1s"
}

@Remote Echo {
    Delay 0.1
}

@Remote2 Echo {
    Delay 0.25
    NotExisting "Hello"
}


Message >Remote {
    Data Test
}

Message >Remote2 {
    Data Test2
}

wait "0.5s"

Message <Remote2 {
    Data (regex)"Test[0-9]"
}

Message <Remote {
    Data Test
}
