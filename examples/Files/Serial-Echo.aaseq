#!/usr/bin/env aaseq

# First endpoint
@Port Serial {
    PortName /dev/ttyUSB1
    BaudRate 9600
    Parity None
    DataBits 8
    StopBits 1
    EOL "CRLF"
}


## Clean any data
#@Port >Discard {
#    In true
#    Out true
#}

# Send line
WriteLine >Port {
    Text Test
}

# Check we received the same line
ReadLine <Port  {
    Text Test
}
