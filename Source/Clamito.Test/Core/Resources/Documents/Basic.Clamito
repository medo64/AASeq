<?xml version="1.0" encoding="utf-8"?>
<Clamito version="1" minVersion="1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://jmedved.com/schema/clamito.xsd">
    <Endpoints>
        <Endpoint name="Endpoint1" displayName="Endpoint 1">
            <Field name="Test" tags=".W W" value="test" />
            <Field name="SubFirst">
                <Field name="SubMiddle" tags="YYY !ZZZ">
                    <Field name="SubLast" value="" />
                </Field>
            </Field>
        </Endpoint>
        <Endpoint name="Endpoint2" displayName="Endpoint 2" protocolName="Dummy" />
    </Endpoints>
    <Interactions>
        <Message name="Request" source="Endpoint1" destination="Endpoint2" description="Something">
            <Field name=".Test" tags="XXX .YYY !.ZZZ" value="Some text" />
            <Field name="Outer">
                <Field name="Inner" tags="YYY !ZZZ">
                    <Field name="A" value="" />
                </Field>
            </Field>
        </Message>
        <Command name="Wait" parameters="10s" description="Error if less than 10s." />
        <Message name="Response" source="Endpoint2" destination="Endpoint1" />
    </Interactions>
</Clamito>